<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="mycartoon.yangqian.com.mycartoon">
    <uses-permission android:name="android.permission.INTERNET"/>

    <application
        android:name=".VideoNewsApplication"
        android:allowBackup="true"
        android:icon="@mipmap/stars1"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".Activity.LeadActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Activity.LogoActivity" />

        <service
            android:name=".service.MediaPlayerService"
            android:enabled="true"
            android:exported="true" />

        <activity android:name=".Activity.HomeActivity"></activity>
        <activity android:name=".tv.activity.ShowActivity"></activity>
        <activity android:name=".tv.activity.WebActivity"></activity>
        <activity android:name=".videoui.base.video.comments.CommentsActivity"/>
    </application>

</manifest>